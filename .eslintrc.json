{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "global-require": "off",
        "class-methods-use-this": "off",
        "default-param-last": "off"
    }
}
