{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "prettier/prettier": "error",
    "global-require": "off",
    "class-methods-use-this": "off",
    "default-param-last": "off"
  }
}
